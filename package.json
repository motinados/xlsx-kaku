{
  "name": "xlsx-kaku",
  "version": "0.17.1",
  "description": "Nodejs library for writing Excel xlsx files.Supports both Client-side and Server-side.",
  "repository": {
    "type": "git",
    "url": "https://github.com/motinados/xlsx-kaku.git"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src",
    "test": "vitest"
  },
  "keywords": [
    "excel",
    "xlsx",
    "writer"
  ],
  "author": "motinados",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "fast-xml-parser": "^4.3.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "uuid": "^9.0.1"
  }
}
